                            </textView>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" editable="NO" text="Calling a bluff" textAlignment="center" selectable="NO" translatesAutoresizingMaskIntoConstraints="NO" id="0sD-iy-Mfj">
                                <rect key="frame" x="60" y="375" width="480" height="33"/>
                                <color key="backgroundColor" white="0.0" alpha="0.0" colorSpace="calibratedWhite"/>
                                <constraints>
                                    <constraint firstAttribute="width" secondItem="0sD-iy-Mfj" secondAttribute="height" multiplier="160:11" id="0ek-5a-PVA"/>
                                </constraints>
                                <color key="textColor" red="0.49609375" green="0.2446758879" blue="0.0" alpha="1" colorSpace="calibratedRGB"/>
                                <fontDescription key="fontDescription" type="system" pointSize="14"/>
