                                <textInputTraits key="textInputTraits" autocapitalizationType="sentences"/>
                            </textView>
                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" editable="NO" text="Calling a card" textAlignment="center" selectable="NO" translatesAutoresizingMaskIntoConstraints="NO" id="cVk-8g-8XK">
                                <rect key="frame" x="60" y="180" width="480" height="28"/>
                                <color key="backgroundColor" white="0.0" alpha="0.0" colorSpace="calibratedWhite"/>
                                <constraints>
                                    <constraint firstAttribute="width" secondItem="cVk-8g-8XK" secondAttribute="height" multiplier="120:7" id="uS9-6g-16R"/>
                                </constraints>
                                <color key="textColor" red="0.49609375" green="0.2446758879" blue="0.0" alpha="1" colorSpace="calibratedRGB"/>
                                <fontDescription key="fontDescription" type="system" pointSize="14"/>
